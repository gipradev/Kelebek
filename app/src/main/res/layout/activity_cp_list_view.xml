<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ui.Report.Cp_One_List.CpListView">
           <LinearLayout
              android:orientation="vertical"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">
               <LinearLayout
                   android:layout_marginTop="10dp"
                   android:orientation="horizontal"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">
                   <LinearLayout
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content">
                       <TextView
                           android:id="@+id/cp_count"
                           android:text="No#"
                           android:gravity="center"
                           android:background="@drawable/round_bg"
                           android:textColor="#FFFFFF"
                           android:textSize="15dp"
                           android:layout_width="25dp"
                           android:layout_height="25dp"/>
                   </LinearLayout>
                   <LinearLayout
                       android:layout_marginLeft="10dp"
                       android:orientation="vertical"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content">
                       <TextView
                           android:text="Name"
                           android:textStyle="bold"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:text="Username"
                           android:textStyle="bold"
                           android:layout_marginTop="10dp"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_marginTop="10dp"
                           android:textStyle="bold"
                           android:text="Joining Date"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_marginTop="10dp"
                           android:textStyle="bold"
                           android:text="Activated Date"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_marginTop="10dp"
                           android:textStyle="bold"
                           android:text="Total BV"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_marginTop="10dp"
                           android:textStyle="bold"
                           android:text="Total PV"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
                   <LinearLayout
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_marginLeft="10dp"
                       android:orientation="vertical">

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="10dp"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="10dp"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="10dp"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="10dp"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="10dp"
                           android:text=":"
                           android:textColor="#000000"
                           android:textSize="15dp" />


                   </LinearLayout>
                   <LinearLayout
                       android:layout_marginLeft="10dp"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">

                       <TextView
                           android:id="@+id/cp_name"
                           android:text="Name"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:text="Username"
                           android:layout_marginTop="10dp"
                           android:id="@+id/cp_username"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>

                       <TextView
                           android:id="@+id/cp_jdate"
                           android:layout_marginTop="10dp"
                           android:text="Joining Date"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>

                       <TextView
                           android:id="@+id/cp_adate"
                           android:layout_marginTop="10dp"
                           android:text="Activated Date"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>

                       <TextView
                           android:id="@+id/cp_bv"
                           android:layout_marginTop="10dp"
                           android:text="Total BV"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:id="@+id/cp_pv"
                           android:layout_marginTop="10dp"
                           android:text="Total PV"
                           android:textColor="#000000"
                           android:textSize="15dp"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
               </LinearLayout>

               <LinearLayout
                   android:background="@drawable/horizontal_line"
                   android:layout_width="match_parent"
                   android:layout_height="30dp">

               </LinearLayout>

           </LinearLayout>



